    "categoryDF = recipeTopTen %>%\n",
    "  select(which(sapply(., class) == \"factor\"))\n",
    "\n",
    "categoryProps = function(x) {\n",
    "    plot = categoryDF %>%\n",
    "        ggplot(aes(x = 1, fill = x)) +\n",
    "        geom_bar(position = \"fill\") +\n",
    "        xlab(\"\") +\n",
    "        ylab(\"proportion\") +\n",
    "        guides(fill=guide_legend(title=\"\"))\n",
    "    print(plot)\n",
    "}\n",
    "categoryDF %>%\n",
    "    walk(categoryProps)\n"
   ]
